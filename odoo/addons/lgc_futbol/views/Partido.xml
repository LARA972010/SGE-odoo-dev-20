<odoo>
  <data>
    
    <record model="ir.ui.view" id="lgc_futbol.partido_tree_view">
      <field name="name">Partido</field>
      <field name="model">lgc_futbol.partido</field>
      <field name="arch" type="xml">
        <tree>
        <field name="equipo_local"></field>
        <field name="equipo_visitante"></field>
          <field name="fecha"/>
          <field name="resultado"/>
          <field name="jugadores" eval="[(4, ref('base.group_user')), (4, ref('jugador_ramos'))]"/>
              
            <tree>
              <field name="name"/>
              <field name="edad"/>
              <field name="posicion"/>
              <field name="equipo_id"/>
            </tree> 
          
          </field>
          
        </tree>
      </field>
    </record>



    <record model="ir.ui.view" id="lgc_futbol.partido_form_view">
      <field name="name">Partido</field>
      <field name="model">lgc_futbol.partido</field>
      <field name="arch" type="xml">
        <form>
          <group>
          <field name="equipo_local"></field>
        <field name="equipo_visitante"></field>
            <field name="fecha"/>
            <field name="resultado"/>
              <field name="jugadores">
                <tree>
                  <field name="name"/>
                  <field name="edad"/>
                  <field name="posicion"/>
                  <field name="equipo_id"/>
                </tree> 
              </field>
           
          </group>
        </form>
      </field>
    </record>


  </data>
</odoo>